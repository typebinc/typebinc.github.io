{
  "tmallOldForceRedirectChromeVersion": "111.0.0.0",
  "tmallOldRedirect": false,
  "tmallNewUIToggle": false,
  "tmallNewUIRollout": 100,
    "aliKRWQueries": ["data.commonModule.currencyCode", "data.currencyComponent.currencyCode", "json.P_MTOP_PCDETAIL.data.result.GLOBAL_DATA.globalData.currencyCode"],
  "worldTaobao": {
    "useLink": true,
    "useClick": true
  },
  "aliDesc": {
    "useDesc": true,
    "descRootQuery": "#product-description",
        "descUrlQuery": ["P_MTOP_PCDETAIL.data.data.productDescComponent.descriptionUrl", "P_MTOP_PCDETAIL.data.result.DESC.pcDescUrl", "descriptionModule.descriptionUrl", "description.fields.detailDesc", "productDescComponent.descriptionUrl"]
  },
  "taoDesc": {
    "useHtmlReplace": true,
    "htmlImageDownloadHeader": {
      "headers": {
        "Accept": "image/avif,image/webp,image/apng,image/svg+xml,image/*,*/*;q=0.8"
      }
    }
  },
  "tmallDesc": {
    "useHtmlReplace": true,
    "htmlImageDownloadHeader": {
      "headers": {
        "Accept": "image/avif,image/webp,image/apng,image/svg+xml,image/*,*/*;q=0.8"
      }
    }
  },
  "iHerbDesc": {
        "querySelectors": [".inner-content", ".collapse-item-content#overview", ".collapse-item-content#details", ".collapse-item-content#product-supplement-facts"]
  },
  "transHtml": {
    "tb": false,
    "ali": false,
    "osee": false,
    "amz": true,
    "vvic": false,
    "rkt": false,
    "iherb": false
  },
  "disabledImageSearchBtn": false,
  "sheinApiVersion": "1.1.8",
  "disabledNaverImageSearchBtn": false,
  "useOseeTracking": true,
  "smartStoreConfig": {
    "skipVideoHls": true
  },
  "vvicLoginCheck": {
    "listButtonCheck": true,
    "itemButtonCheck": true
  },
  "useAutoPhotoReview": true,
  "photoReviewConfig": {
    "score": 3,
    "negativeWordCount": 2
  },
  "templateScrap": {
    "v_taobao": "1.1.117",
    "v_tmall": "1.1.117",
    "v_vvic": "1.1.117",
    "v_rakuten": "1.1.117",
    "v_ali": "1.1.117"
  },
  "scrapConfig": {
    "checkDuplication": true,
    "useOffscreen": true,
    "excludedImageList": [
      "img.alicdn.com/imgextra/i1/O1CN011GDmVH1PCjsNLVMtP_!!6000000001805-2-tps-750-880.png",
      "https://img.alicdn.com/imgextra/i3/O1CN01MsWlBa29wVnysxUvn_!!6000000008132-2-tps-750-880.png"
    ]
  },
  "taobaoConfig": {
    "scrap": {
      "useDynamicProperty": true,
      "dynamicPropertyPaths": [["stateNode", "state", "detail"]],
      "dynamicMaxDepth": 30,
      "useImageHeader": false
    },
    "trigger": {
      "useDescTrigger": true,
      "descClassName": "[class^=BasicContent]"
    },
    "cart": {
      "querySelectors": {
                 "container": [
                     "#listContainer_1"
                 ],
                 "product": [
                     "a[class*='imageContainer--']"
                 ]
      },
      "maxAttempts": 30,
      "attemptDelayMs": 100
    },
    "seller_store": {
      "querySelectors": [
        {
          "container": ["div[data-spm='product_shelf']>div"],
          "product": ["a[class^=linkWrapperArea--]"]
        },
        {
          "container": [".J_TItems"],
          "product": ["a.J_TGoldData"]
        },
        {
          "container": [".tshop-pbsm"],
          "product": ["dl.item>dt.photo>a"]
        }
      ],
      "urlRegex": [
        "^https:\\/\\/\\w+(?:\\.world)?\\.(?:taobao|tmall)\\.com\\/shop\\/",
        "^https:\\/\\/shop\\w+(?:\\.world)?\\.(?:taobao|tmall)\\.com\\/",
        "^https:\\/\\/\\w{2,}(?:\\.world)?\\.(?:taobao|tmall)\\.com\\/search\\.htm",
        "scene=taobao_shop",
        "\\.(?:taobao|tmall)\\.com\\/category-\\d+\\.htm",
        "^https:\\/\\/\\w{10,}\\.(?:taobao|tmall)\\.com\\/"
      ],
      "maxAttempts": 30,
      "attemptDelayMs": 100
    },
    "list": {
      "querySelectors": [
        {
          "container": ["div[class^='detailHoverCardWrapper']"],
          "product": ["a[class^='detailHoverCard']"],
          "type": "product_dom_width_hover_expansion"
        },
        {
          "container": ["[class^='leftContent']"],
          "product": ["[class*='search-content-col']"],
          "type": "common_list"
        },
        {
          "container": ["div.tb-pick-feeds-container"],
          "product": ["div.tb-pick-content-item a.item-link"],
          "type": "main_page_list"
        },
        {
          "container": ["div#mainsrp-itemlist"],
          "product": ["div.pic-box"],
          "type": "old_list"
        }
      ]
    }
  },
  "tmallConfig": {
    "scrap": {
      "useDynamicProperty": true,
      "dynamicPropertyPaths": [["stateNode", "state", "detail"]],
      "dynamicMaxDepth": 30,
      "useImageHeader": false
    },
    "trigger": {
      "useDescTrigger": true,
      "descClassName": "[class^=BasicContent--root]"
    }
  },
  "amazonConfig": {
    "list": {
      "querySelectors": [
        {
          "container": ["div.s-main-slot"],
          "product": [
            "div.s-product-image-container a[href*='/dp/']",
            "div.s-product-image-container a[href*='/gp/']",
            "div.s-product-image-container a[href*='-spons%']"
          ],
          "type": "global"
        },
        {
          "container": [
            "div[class*='_octopus-search-result-card_style_apbSearchResultsContainer__']"
          ],
          "product": ["a.octopus-pc-item-link, a.s-no-outline"],
          "type": "japen"
        },
        {
          "container": [
            ".stores-column div[class*='ProductGrid__gridContainer']"
          ],
          "product": [
            "li[class*='ProductGridItem__itemOuter'] a[class*='ProductGridItem__overlay']"
          ],
          "type": "seller_store"
        }
      ],
      "maxAttempts": 30,
      "attemptDelayMs": 100
    },
    "template": {
      "salePrice": [
        "#twister-plus-price-data-price"
      ],
      "thumbnailImage": {
        "container":"#altImages",
        "idx": 0
      },
      "optionImage": {
        "swatch": "#tp-inline-twister-dim-values-container",
        "select": "form#twister"
      }
    }
  },
  "rakutenConfig": {
    "scrap": {
      "dynamicPropertyPaths": ["api.itemInfoSku"],
      "dynamicMaxDepth": 50,
      "dynamicEssentialFields": ["itemId"],
      "thumbnailSelectors": ["div[irc='Image'] img"]
    }
  },
  "rakutenFashionConfig": {
    "scrap": {
      "thumbnailSelector": ".item-images-thumbnail-item",
      "stockInfoSelector": ".item-sku-actions-info",
      "descTableSelector": ".item-detail-description",
      "sizeTableSelector": ".item-detail-size-info"
    }
  }
}
